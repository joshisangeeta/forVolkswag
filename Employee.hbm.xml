<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.emp.Employee" table="emptable">
	  <id name="id" type="int" column="eid">
	   <generator class="assigned"/>
	  </id>

	  <property name="name">
		 <column name="ename" />
	  </property>
	  <property name="sal">
		<column name="esal"/>
	  </property>	  
   </class>
</hibernate-mapping>